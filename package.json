{
  "name": "employee-directory",
  "version": "1.0.0",
  "description": "A Hr react and node application for managing employees in an organisation",
  "main": "index.js",
  "scripts": {
    "dev:server": "webpack-dev-server --hot --config ./config/webpack.dev.js",
    "start:client": "webpack --config ./config/webpack.prod.js",
    "start:server": "npm run build && node ./server/dist/index.js",
    "start:devserver": "nodemon server/src/index.js --exec @babel-node --presets @babel/preset-env",
    "build": "babel ./server/src -d ./server/dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:init": "cd ./server/src && npx sequelize-cli init && cd ..../",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "deploy": "npm run start:client && gh-pages -d ./client/public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/successgilli/Employee-directory.git"
  },
  "author": "A.N Gilbert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/successgilli/Employee-directory/issues"
  },
  "homepage": "https://github.com/successgilli/Employee-directory#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-register": "^6.26.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "redux-devtools-extension": "^2.13.8",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.0.3",
    "proptypes": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.3"
  }
}
